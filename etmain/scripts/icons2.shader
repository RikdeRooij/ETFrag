icons/iconw_mp40_1
{
	nocompress
	nomipmaps
	nopicmip
	{
		map icons/iconw_mp40_1.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen Vertex
	}
}


